
@{
    ViewBag.Title = "Show";
}

<h2>Show</h2>

<table border="1">
    <thead>
        <tr>
            <th>
                First Name
            </th>
            <th>
            </th>
            <th>
                Last Name
            </th>
            <th>

            </th>
            <th>
                Employee Number
            </th>
            <th>

            </th>
            <th>
                Hire Date
            </th>
            <th>

            </th>
            <th>
                Salary
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var teacher in Model)
        {
            <tr>
                <td>
                    @teacher.TeacherFName
                </td>
                <td>
                </td>
                <td>
                    @teacher.TeacherLName
                </td>
                <td>
                </td>
                <td>
                    @teacher.EmployeeNumber
                </td>
                <td>
                </td>
                <td>
                    @teacher.HireDate
                </td>
                <td>
                </td>
                <td>
                    @teacher.Salary
                </td>
            </tr>
        }
    </tbody>
</table>